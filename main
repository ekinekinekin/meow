     public static void clearConsole() throws InterruptedException { // Method for clearing console
        try
        {
            new ProcessBuilder("cmd", "/c", "cls").inheritIO().start().waitFor();
        }
        catch (java.io.IOException ioe)
        {
            ioe.printStackTrace();
        }
        System.out.flush();
    }
    
